# Database connection string
# This default value works with the provided docker-compose.yml
DATABASE_URL=postgresql://user:password@localhost:5432/mydatabase

# Google AI API Key
# Get yours from https://aistudio.google.com/app/apikey
GOOGLE_API_KEY="YOUR_GOOGLE_AI_API_KEY"

# LangSmith Tracing (Optional)
# Set to "true" to enable tracing and provide your API key
# Get yours from https://smith.langchain.com/
LANGCHAIN_TRACING_V2="true"
LANGCHAIN_API_KEY="YOUR_LANGSMITH_API_KEY"